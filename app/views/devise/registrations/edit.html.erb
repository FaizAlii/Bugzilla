<div class="row justify-content-center">
  <%= render "devise/registrations/form", resource: resource, request_type: 'put', form_type: 'Update' %>

  <%= button_to "Cancel my account", registration_path(resource_name), data: { confirm: "Are you sure?" }, method: :delete, class: 'btn btn-danger btn-cancel' %>

  <%= link_to "Back", :back %>
</div>
